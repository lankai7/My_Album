            <string notr="true">/* 统一基本外观 */
QDateEdit, QTimeEdit, QDateTimeEdit {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 rgba(255,255,255,0.02),
                                stop:1 rgba(255,255,255,0.01));
    border: 0px solid transparent;         /* 去除默认边框 */
    border-radius: 6px;                    /* 圆角 */
    padding: 8px 28px 8px 12px;            /* 留出右侧空间给箭头按钮 */
    color: #FFFFFF;                        /* 文本颜色 */
    font: 13px &quot;Microsoft YaHei&quot;;
}

/* 默认底部线条 */
QDateEdit, QTimeEdit, QDateTimeEdit {
    border-bottom: 1px solid rgba(255,255,255,0.12);
}

/* 悬停时微微提亮 */
QDateEdit:hover, QTimeEdit:hover, QDateTimeEdit:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 rgba(255,255,255,0.035),
                                stop:1 rgba(255,255,255,0.02));
    border-bottom: 1px solid rgba(255,255,255,0.22);
}

/* 聚焦时高亮下边框 */
QDateEdit:focus, QTimeEdit:focus, QDateTimeEdit:focus {
    outline: none;
    border-bottom: 3px solid #1E90FF; /* 高亮色 */
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 rgba(255,255,255,0.04),
                                stop:1 rgba(255,255,255,0.02));
}

/* 禁用状态 */
QDateEdit:disabled, QTimeEdit:disabled, QDateTimeEdit:disabled {
    color: rgba(255,255,255,0.25);
    background: rgba(255,255,255,0.01);
    border-bottom: 1px dashed rgba(255,255,255,0.06);
}

/* 下拉按钮区域（小箭头） */
QDateEdit::drop-down, QTimeEdit::drop-down, QDateTimeEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    width: 20px;
    border: none;
    background: transparent;
    margin-right: 4px;
}

/* 下拉按钮悬停效果 */
QDateEdit::drop-down:hover, QTimeEdit::drop-down:hover, QDateTimeEdit::drop-down:hover {
    background: rgba(255,255,255,0.08);
    border-radius: 4px;
}

/* 小箭头样式 */
QDateEdit::down-arrow, QTimeEdit::down-arrow, QDateTimeEdit::down-arrow {
 /* 这里替换成你自己的浅色小箭头图标 */
    width: 12px;
    height: 12px;
}

